      </TableView>
      <TextField fx:id="tfrech" layoutX="268.0" layoutY="64.0" />
      <Button fx:id="btnret" layoutX="62.0" layoutY="335.0" mnemonicParsing="false" onAction="#retour" text="Retour" />
      <Button fx:id="btnsupp" layoutX="260.0" layoutY="335.0" mnemonicParsing="false" onAction="#supprimer" text="Supprimer" />
      <Button fx:id="btnmod" layoutX="466.0" layoutY="335.0" mnemonicParsing="false" onAction="#modifier" text="Modifier" />
