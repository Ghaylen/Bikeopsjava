          <TableColumn fx:id="idc" prefWidth="75.0" text="Id Categorie" />
          <TableColumn fx:id="namec" prefWidth="75.0" text="Nom Categorie" />
            <TableColumn fx:id="descc" prefWidth="75.0" text="Description Categorie" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
